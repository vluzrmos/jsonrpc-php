{
    "name": "vluzrmos/jsonrpc",
    "description": "JSON-RPC 2.0 Client and Server implementation for PHP 5.6+",
    "keywords": ["jsonrpc", "json-rpc", "json rpc", "rpc", "client", "server", "http", "curl"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vagner do Carmo",
            "email": "vluzrmos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Vluzrmos\\JsonRPC\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
